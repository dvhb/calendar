.CalendarMonthHeader
	sf()
	ff(light_f, 24)
	color #000
	border-bottom 1px solid #e9eaeb
	margin-bottom 17px
	margin-left 10px
	height 27px

.CalendarMonthHeaderRight
	@extend .CalendarMonthHeader

	margin-left 0px
	margin-right 10px

.CalendarMonthHeaderCurrentMonth
	background #fff
	margin-bottom -5px
	display inline
	line-height 28px
	float right
	padding-left 5px
	padding-right 2px
	position relative

	sup
		ff(light_f, 14)
		position absolute
		right 100%
		top 0


.CalendarMonthHeaderSiblingMonth
	float left
	background #fff
	ff(medium, 14)
	background-image: url('data:image/png;base64,');
	background-position: 0 center
	background-repeat no-repeat
	padding-left 10px
	line-height 35px
	padding-right 5px

.CalendarMonthHeaderCurrentMonthLeft
	@extend .CalendarMonthHeaderCurrentMonth

	padding-left 0px
	padding-right 5px
	float left

	sup
		right auto
		left 100%

.CalendarMonthHeaderSiblingMonthRight
	@extend .CalendarMonthHeaderSiblingMonth

	float right
	background-position: right center
	padding-left 5px
	padding-right 10px


