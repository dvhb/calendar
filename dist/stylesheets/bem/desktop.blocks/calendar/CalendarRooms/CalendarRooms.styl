.CalendarRoomsTable
	width 100%
	border-collapse separate
	border-spacing 1px
	background #fff
	border-left 1px solid #e9eaeb
	border-right 1px solid #e9eaeb
	border-bottom 1px solid #adadad

.CalendarRoomsTableHead
	ff(medium, 11)
	text-align left
	background #e6e6e6
	vertical-align top
	padding 6px 10px
	border-bottom 1px solid #b6b6b6

.CalendarRoomsTableHeadIcon
	display inline-block
	width 16px
	height 14px
	background-repeat no-repeat
	background-position 0 center
	margin 5px 5px -5px 0

.CalendarRoomsTableHeadIconProjector
	@extend .CalendarRoomsTableHeadIcon

	background-image: url('data:image/png;base64,');

.CalendarRoomsTableHeadIconComp
	@extend .CalendarRoomsTableHeadIcon

	background-image: url('data:image/png;base64,');

.CalendarRoomsTableHeadIconNotebook
	@extend .CalendarRoomsTableHeadIcon

	background-image: url('data:image/png;base64,');

.CalendarRoomsCell
	@extend .CalendarWeekRowCell

	background-color #f0f0f0

.CalendarRoomsCellTime
	@extend .CalendarWeekRowCellTime

	background-color #e6e6e6