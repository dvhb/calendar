.CalendarWeekOverlay
	height 0px
	margin 0
	padding 0

.CalendarWeekOverlayItem
	position absolute
	background #d0dde4
	background rgba(0,120,193,0.1)
	border-left 2px solid #0078c1
	list-style none
	font-size 11px
	color #256c97
	padding-left 2px
	padding-right 20px
	overflow hidden
	line-height 15px
	cursor pointer

.CalendarWeekOverlayItemActive
	z-index 1001
	cursor pointer

.CalendarWeekOverlayItemBlue
	background:rgba(0,120,193,0.2);
	cursor pointer

.CalendarWeekOverlayItemPink
	background:rgba(62,48,165,0.1);
	border-left-color #3e30a5
	cursor pointer

.CalendarWeekOverlayItemPurple
	background:rgba(48,62,165,0.1);
	border-left-color #303ea5
	cursor pointer

.CalendarWeekOverlayItemGreen
	background:rgba(48,144,165,0.1);
	border-left-color #3090a5
	cursor pointer

.CalendarWeekOverlayItemCutted
	@extend .CalendarWeekOverlayItem

	white-space nowrap
	text-overflow ellipsis
	cursor pointer


.CalendarWeekOverlayItemInvite
	position absolute
	top 2px
	right 2px
	width 12px
	height 10px
	background-image: url('data:image/png;base64,');

.CalendarWeekOverlayItemInviteAccepted
	@extend .CalendarWeekOverlayItemInvite

	width 16px;
	background-position right 0

.CalendarWeekOverlayItemNewEvent
	color #194763
	ff(medium, 11)

.CalendarWeekOverlayItemNewEventCutted
	@extend .CalendarWeekOverlayItemNewEvent

	overflow hidden
	text-overflow ellipsis

.CalendarWeekOverlayItemDrag
	height 10px
	position absolute
	bottom 0px
	left 0px
	width 100%
	cursor row-resize
	text-align center


.ml-ellipsis
	overflow hidden
	height 100%
	width:100%


.ml-ellipsis > .ml-ellipsis-before
	float left
	width 1px
	height 100%


.ml-ellipsis > .ml-ellipsis-content
	float right
	width calc(100% - 1px)


.ml-ellipsis > .ml-ellipsis-after
	white-space nowrap
	box-sizing content-box

	float right
	position relative
	top -13px
	left 100%
	width 3em
	margin-left -3em
	padding-right 1px

	text-align right
	background #d0dde4
	background linear-gradient(to right, rgba(208,221,228,0) 0%,rgba(208,221,228,1) 45%,rgba(208,221,228,1) 100%)
	height 11px
	line-height 9px

	span
		display inline-block
		line-height 10px