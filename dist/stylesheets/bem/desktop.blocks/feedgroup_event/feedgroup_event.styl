.feedgroup_event_wrap
    position absolute
    left 50%
    right 50%
    margin-top 100px
    z-index 10001

.feedgroup_event
    width 360px
    margin-left -180px

.feedgroup_event_label
    display block
    sf()
.feedgroup_event_label__left
    width 120px
    float left
.feedgroup_event_label__right
    width 180px
    float right
    margin-right -20px
.feedgroup_event_label__rightplace
    margin-right -55px
.feedgroup_event_time_from
    width 105px
    display inline-block
    margin-right 10px
    ff(normal,14)
    color #333
    input
        width 85px
.feedgroup_event_time_to
    width 110px
    display inline-block
    ff(normal,14)
    color #333
    input
        width 85px

.feedgroup_event_webinar
    margin-bottom 15px
    sf()

.feedgroup_event_placelabel
    margin-bottom 10px
    sf()
.feedgroup_event_people_add
    padding 19px 0px
.feedgroup_event_people
    padding 0px 0px 0px 0px


.feedgroup_event_people_item
    padding 9px 29px 0px 29px
    border-bottom 1px solid #e9eaeb
    display block
    cursor pointer
    position relative
    sf()
    &:last-child
        border-bottom none
    &:hover
        background url('data:image/png;base64,') repeat left top
        .feedgroup_event_people_item_delete
            display block !important

.feedgroup_event_people_item_delete
    position absolute
    display none
    width 16px
    height 16px
    left 5px
    top 28px
.feedgroup_event_people_item_avatar_wrap
    display table-cell


.feedgroup_event_people_item_text
    display table-cell
    vertical-align top
    width 100%

.feedgroup_event_people_item_name
    ff(medium,14)
    color #333
    margin-bottom 2px


.feedgroup_event_people_item_depart
    ff(medium,11)
    color #7a7e80
    margin-bottom 2px

.feedgroup_event_people_item_status
    ff(medium,11)
    color #7a7e80
    margin-bottom 2px

.feedgroup_event_people_item_status_ohra
    color #a98107
.feedgroup_event_people_item_status_red
    color #a92907
.feedgroup_event_people_item_status_blue
    color #256c97


.feedgroup_event_notice{
    ff(bold, 11)
    color #808080
    text-transform uppercase
}