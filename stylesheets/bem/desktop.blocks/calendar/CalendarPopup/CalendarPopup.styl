.CalendarPopup
	position absolute
	width 382px
	background #FFF
	border 1px solid #e9e9ea
	border-bottom-color #adadad
	z-index 1001

	&:after
		content ''
		display block
		position absolute
		background url("data:image/png;base64,") no-repeat 0 0
		width 11px
		height 21px
		top 23px
		left -11px

.CalendarPopupRight
	@extend .CalendarPopup

	&:after
		left auto
		right -11px
		background-position right 0

	.CalendarPopupClose
		right auto
		left -15px

.CalendarPopupClose
	position absolute
	top 30px
	width 30px
	height 30px
	right -15px
	background url("data:image/png;base64,") no-repeat center center;
	cursor pointer
	z-index 1002

.CalendarPopupGroup
	border-bottom 1px solid #e9eaeb
	padding 10px 28px

.CalendarPopupHeader
	color #0078c1
	ff(light_f, 24)
	line-height 30px
	padding 13px 0 16px

.CalendarPopupHeaderDescr
	color #666666
	ff(bold, 11)
	line-height 18px



.CalendarPopupMan
	sf()
	margin -10px -28px
	padding 10px 28px
	position relative

.CalendarPopupManRemovable
	cursor pointer

	&:hover
		background url("data:image/png;base64,") repeat left top
		.CalendarPopupManRemoveButton
			display block


.CalendarPopupManRemoveButton
	position: absolute;
	display: none;
	width: 16px;
	height: 16px;
	left: 5px;
	top: 28px;
	background url("data:image/png;base64,") no-repeat left center

.CalendarPopupManAvatar
	float left
	margin-right 11px

.CalendarPopupManName
	line-height 18px
	overflow hidden
	a
		ff(medium, 14)
	strong
		color #fff
		ff(bold, 11)
		background #969696
		text-transform uppercase
		padding 0 3px
	p
		color #7a7e80
		ff(medium, 11)
		line-height 16px
		margin-bottom 2px

.CalendarPopupAdress
	font-size 14px
	line-height 18px
	padding 6px 0 10px

.CalendarPopupAdressPlace
	ff(bold, 11)
	color #66aeda
	text-transform uppercase
	background-image url('data:image/png;base64,')
	padding-left 20px
	background-repeat no-repeat
	background-position 0 center
	margin-top 6px

.CalendarPopupInfoText
	font-size 13px
	line-height 18px
	padding-top 10px
	margin-bottom -3px

	ol
		margin 0 0 9px 20px
		padding 0

	li
		margin-bottom 4px

.CalendarPopupMembers
	sf()
	margin-right -20px

	.CalendarPopupManAvatar
		margin-bottom 8px

.CalendarPopupMembersScroll
	overflow auto
	max-height 248px

.CalendarPopupMembersUnconfirmed
	ff(bold, 11)
	color #666666

.CalendarPopupRelativeFormItem
	position relative

	.picker
		width 100%

	.picker__holder
		max-width 100%

	.autocomplete
		width 100%

.CalendarPopupGroupGreyBorder
	border-bottom 1px solid #e9eaeb


.CalendarPopupAllMembers
	font-size 11px
	color #666666
	a
		font-size 16px