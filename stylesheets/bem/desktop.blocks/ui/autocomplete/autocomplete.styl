.autocomplete_block
    margin-top 50px
    padding-top 20px
    padding-right 200px !important
    position relative
.autocomplete
    border 1px solid #619dc2
    margin-top -11px
    overflow hidden
    position absolute
    background-color #FFF
    z-index 999
.autocomplete_item
    padding 10px 20px
    border-bottom 1px solid #e5e5e5
    display table
    cursor pointer
    sf()
    &:last-child
        border-bottom none

.autocomplete_item_avatar_wrap
    display table-cell
.autocomplete_item_avatar
    margin-right 8px
    width 54px
    height 54px
    border-radius 50%
    padding 2px 2px 2px 2px
    sf()
    a
        width 50px
        height 50px
        display block
        sf()
        img
         border-radius 50%
         width 50px
         height 50px
.autocomplete_item_avatar_yellow
    background #F3E865
.autocomplete_item_avatar_rose
    background #ed7e95

.autocomplete_item_text
    display table-cell
    vertical-align top
    width 100%
    padding-left 20px

.autocomplete_item_name
    ff(medium,14)
    color #333
    margin-bottom 2px


.autocomplete_item_depart
    ff(medium,11)
    color #7a7e80
    margin-bottom 2px

.autocomplete_item_status
    ff(medium,11)
    color #7a7e80
    margin-bottom 2px

.autocomplete_item_status_ohra
    color #a98107
.autocomplete_item_status_red
    color #a92907
.autocomplete_item_status_blue
    color #256c97


.autocompleteWrapper
    position relative

.autocompleteTop
    .autocomplete
        bottom 100%
        margin-bottom -11px