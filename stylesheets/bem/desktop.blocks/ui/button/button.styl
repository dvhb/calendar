@import 'buttonBlueIcon'
@import "setting"
@import '_icon/_icon.styl'
@import 'ButtonIconLeft.styl'

.button
  display inline-block
  text-align center
  color grey
  background light-grey
  border-top 1px solid transparent
  border-left none
  border-right 2px solid dark-grey
  border-bottom 2px solid dark-grey
  width auto
  padding 7px 13px
  text-shadow 0 1px 0 rgba(255, 255, 255, 0.5)
  margin 1px -4px 1px 0px
  ff(bold,13)
  cursor pointer
  &:hover
    background-color #efefef;
  &:active
    background-color #f0f0f0
    border-top 1px solid transparent
    border-right 1px solid #b6b6b6
    border-bottom 1px solid #b6b6b6
    border-left 0
    padding 7px 14px 8px 13px

.button__sticky_right
  border-right 1px solid dark-grey
  &:active
    padding-right 13px !important

.button_cancel
  display block
  position absolute
  right -16px
  top 16px
  width 32px
  height 32px
  border none
  background url('data:image/png;base64,') no-repeat center center
.button__right
  float right
  margin-top 4px
  margin-right 3px

.button__smallright
  float right
  margin-top 12px !important
  margin-right 3px

@import '__colors/button__colors'
@import '__sizes/button__sizes'

.button__dropdown
  text-align left
  padding 16px 7px 16px 14px
  border-top 1px solid #d5d5d5 !important
  margin 0 -4px 0 0px
  background-color #f0f0f0 !important

.button__icon
  padding-left 40px
  border-top 1px solid transparent !important
  &:active
    padding-left 40px

.button__icon__small
  padding-left 20px
  border-top 1px solid transparent !important
  &:active
    padding-left 20px

.button__like
  display block
  position relative
  float right
  width 72px
  color grey
  background light-grey
  border-top 1px solid transparent
  border-left none
  border-right 2px solid dark-grey !important
  border-bottom 2px solid dark-grey !important
  height 34px
  padding 0px 0px

.button__like_onlynumber
  width auto
  &:active
    width auto
    padding 0px 0px

.button__like_left
  width 36px
  height 34px
  display block
  position absolute
  top 0
  left 0
  border-top 1px solid transparent
  border-left none
  border-right 1px solid transparent !important
  border-bottom 1px solid transparent !important
  &:active
    margin-top -1px
    background-position center 45%
    background-color #f0f0f0
    border-top 1px solid transparent !important
    border-right 1px solid transparent !important
    border-bottom 1px solid #b6b6b6 !important

.button_icon__like_a
  &.button__like_left
    &:active
      margin-top -1px
      background-position center 40%
      background-color light-grey
      border-top 1px solid transparent
      border-left none
      border-right 1px solid transparent !important
      border-bottom 2px solid #b6b6b6 !important
.button__like_number

  width 34px
  height 33px
  padding 7px 0 0 0

.button__like_right
  border-left 1px dotted #333
  width 34px
  height 31px
  padding 7px 0 0 0px
  position absolute
  top 0
  right 0
  text-align center
  ff(bold,13)
  color #000


.button__buttonicon

  border-top 1px solid transparent !important

.button__icon_plus
  background url('data:image/png;base64,') no-repeat 1em center

.button__smallicon
    ff(bold,11)
    border none !important
    color #86b5d2
    background-position left top !important
    padding-left 20px
    padding-top 0px
    margin-top 5px
    &:hover
      background-color transparent

.button__mediumicon
    ff(bold,13)
    border none !important
    color blue
    background-position left center !important
    padding 3px 20px 8px 25px
    &:hover
      background-color transparent
    &:active
      padding 3px 20px 8px 25px




.button__textinput
    float left
    box-sizing border-box
    background #FFF
    border 1px solid #d6d6d6
    padding-left 32px
    height 50px
    min-height 50px
    max-height 50px
    ff(normal,14)
    margin 0
    margin-top 32px
    margin-bottom 32px
    display inline-block
    width 100%
    text-align left
    color rgba(0,0,0,0.4)
.button__topborder
  border-top 1px solid #959595 !important

.button__textinput_icon
    width 44px
    height 44px
    border-left 1px solid #e9eaeb
    border-top none
    border-right none
    border-bottom none
    position absolute
    right 0
    top 11px
@import 'button.ie.styl'


.button__noleftpadding
  padding-left 0px
  border none
  margin 0

  &:active
    padding-left 0px
    border none
    margin 0
    padding-bottom 7px

/********* CalendarButtons *********/

.ButtonGroup
  & > *
    float left
    cf()

.ButtonCalendar
  width 44px
  height 44px
  background #e3e3e3
  position relative
  border-bottom 1px solid #b6b6b6
  border-right 1px solid #b6b6b6
  display block
  cursor pointer
  background-position center 13px
  background-repeat no-repeat
  z-index 1

  &:after
    position absolute
    content: ''
    left 1px
    top 1px
    width 100%
    height 100%
    border-bottom 1px solid #b6b6b6
    border-right 1px solid #b6b6b6
    display block

  &:active,
  &.active
    border-top 1px solid #FFF
    border-left 1px solid #fff
    border-bottom none
    border-right none
    background-position center -68px
    background-color #f0f0f0

    &:after{
      left 0
      top 0
    }

  &:hover
    background-position center -68px
    background-color #f0f0f0

.ButtonCalendarExpand
  @extend .ButtonCalendar

  background-image: url('data:image/png;base64,');

.ButtonCalendarWeek
  @extend .ButtonCalendar

  background-image: url('data:image/png;base64,');

.ButtonCalendarDaily
  @extend .ButtonCalendar

  background-image: url('data:image/png;base64,');

.ButtonCalendarReserve
  @extend .ButtonCalendar

  background-image: url('data:image/png;base64,');

.ButtonCalendarCollapse
  @extend .ButtonCalendar

  background-image: url('data:image/png;base64,');




